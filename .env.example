# Copy this file to .env and edit values before running Docker Compose.

# REQUIRED: set a long random key for admin-protected endpoints.
# Example generator: openssl rand -hex 32
RC_ADMIN_KEY=replace-with-64-hex-chars

# Domain used in TLS certificate CN/SAN and Nginx server_name.
# For local test keep localhost.
RUSTCHAIN_DOMAIN=localhost

# SQLite DB path inside container (persisted via rustchain_data volume)
RUSTCHAIN_DB_PATH=/data/rustchain_v2.db
