# RustChain Telegram Wallet Bot Configuration
# Copy to .env and fill in your values

# Required: Get from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=YOUR_BOT_TOKEN_HERE

# RustChain node API URL
RUSTCHAIN_API=https://rustchain.org

# SSL verification (set to true for production with valid certs)
RUSTCHAIN_VERIFY_SSL=false

# Keystore directory (default: ~/.rustchain/telegram_wallets)
# KEYSTORE_DIR=/path/to/wallets
